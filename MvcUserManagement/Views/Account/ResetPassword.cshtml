@using MvcDataManager
@model MvcDataManager.Model.ResetPassword
@{
    ViewBag.Title = "ChangePassword";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/ChangePasswordPanel.css" rel="stylesheet" />

@using (Html.BeginForm())
{
    <div>
        @Html.ChangePasswordControlForModel(m=>m.UserName, m=>m.NewPassword, m=> m.ConfirmPassword, "Login", "Account")
    </div>
}

