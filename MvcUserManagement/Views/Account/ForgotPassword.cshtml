@using MvcDataManager
@model MvcDataManager.Model.ForgotPassword

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/ForgotPasswordPanel.css" rel="stylesheet" />

@using (Html.BeginForm("ForgotPassword", "Account"))
{
    <div>
        @Html.ForgotPasswordControlForModel(m => m.UserName, m => m.SecurityQuestionId, Model.SecurityQuestions, m => m.Answer, "CancelForgotPassword", "Account")
    </div>
}